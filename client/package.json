{
  "name": "cedar",
  "productName": "cedar",
  "version": "0.5.30",
  "license": "MIT",
  "author": {
    "name": "Garrett Newman",
    "email": "garrett.s.newman@gmail.com",
    "url": "https://github.com/gasnew"
  },
  "repository": "github:gasnew/cedar",
  "main": "public/electron-main.js",
  "homepage": "./",
  "dependencies": {
    "@blueprintjs/core": "^3.28.1",
    "@blueprintjs/select": "^3.13.4",
    "@feathersjs/feathers": "^4.5.3",
    "@feathersjs/socketio-client": "^4.5.4",
    "@reduxjs/toolkit": "^1.3.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.157",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "electron-log": "^4.3.1",
    "electron-updater": "^4.3.5",
    "js-base64": "^3.4.4",
    "lodash": "^4.17.19",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "socket.io-client": "^2.3.0",
    "typescript": "~3.7.5"
  },
  "build": {
    "appId": "com.gasnew.cedar",
    "productName": "cedar",
    "copyright": "Copyright Â© 2021 gasnew",
    "mac": {
      "category": "public.app-category.utilities",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "win": {
      "publisherName": "Developer ID Application: Garrett Newman (Z986S6NQ78)"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "afterSign": "scripts/notarize.js",
    "extends": null
  },
  "scripts": {
    "start": "nf start -p 3000",
    "start-react": "BROWSER=none REACT_APP_VERSION=$npm_package_version react-scripts start",
    "start-electron": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "build": "REACT_APP_VERSION=$npm_package_version react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "build-electron-app": "electron-builder",
    "dist-local": "yarn build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "remove-unused-imports": "tslint --config tslint-remove-unused-imports.json --fix --project ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "10.1.1",
    "electron-builder": "^22.9.1",
    "electron-notarize": "^1.0.0",
    "foreman": "^3.0.1",
    "tslint": "^6.1.3",
    "tslint-etc": "^1.13.7",
    "wait-on": "^5.0.0"
  }
}
