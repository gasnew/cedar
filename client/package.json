{
  "name": "cedar",
  "productName": "cedar",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Garrett Newman",
    "email": "garrett.s.newman@gmail.com",
    "url": "https://github.com/gasnew"
  },
  "license": "MIT",
  "main": "public/electron-main.js",
  "homepage": "./",
  "dependencies": {
    "@blueprintjs/core": "^3.28.1",
    "@blueprintjs/select": "^3.13.4",
    "@feathersjs/feathers": "^4.5.3",
    "@feathersjs/socketio-client": "^4.5.4",
    "@reduxjs/toolkit": "^1.3.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.157",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "js-base64": "^3.4.4",
    "konva": "^7.0.2",
    "lodash": "^4.17.19",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-konva": "^16.13.0-3",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "socket.io-client": "^2.3.0",
    "typescript": "~3.7.5"
  },
  "build": {
    "appId": "com.electron.cedar",
    "productName": "cedar",
    "copyright": "Copyright Â© 2020 gasnew",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extends": null
  },
  "scripts": {
    "start": "nf start -p 3000",
    "start-react": "BROWSER=none react-scripts start",
    "start-electron": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "dist": "yarn build && electron-builder -mw",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "10.1.1",
    "electron-builder": "^22.8.0",
    "foreman": "^3.0.1",
    "wait-on": "^5.0.0"
  }
}
